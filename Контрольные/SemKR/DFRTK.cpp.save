struct Rect * transform (const struct Rect * src) {
    int xl = (src->lt).x;
    int yl = (src->lt).y;
    int xr = (src->rb).x;
    int yr = (src->rb).y;

    struct Rect * rect = (struct Rect *)malloc(sizeof(struct Rect));
    (rect->lt).x = xl - yl + yr;
    (rect->rb).x = xl;
    (rect->lt).y = yr + xr - xl;
    (rect->rb).y = yr;

    return rect;
}
